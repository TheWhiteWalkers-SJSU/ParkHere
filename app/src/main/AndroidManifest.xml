<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thewhitewalkers.parkhere">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomepageActivity" />
        <activity android:name=".InboxActivity" />
        <activity android:name=".ViewRequestActivity" />
        <activity android:name=".CreateListingActivity" />
        <activity android:name=".SearchListingActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".ViewListingActivity" />
        <activity android:name=".CreateRequestActivity" />
        <activity android:name=".UpdateListingActivity" />
        <activity android:name=".RatingActivity" />
        <activity android:name=".ViewRatingsActivity" />
        <activity android:name=".TabbedInboxActivity" />
    </application>

</manifest>